-- NotificationBilling.Notifications definition

CREATE TABLE `Notifications` (
  `customerNumber` varchar(12) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT 'Number of Customer',
  `idAccountTwitter` varchar(30) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT 'Account Twitter Identifer',
  `accountTwitter` varchar(100) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT 'Twitter Account User',
  `dateCreated` varchar(10) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT 'Creation Date',
  `status` int(11) DEFAULT NULL COMMENT 'Twitter Account Status'
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

